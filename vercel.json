{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/vipMembers.js",  // Corrected to the vipMembers.js function
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/vipMembers",    // Direct the vipMembers route to the new file
      "dest": "/api/vipMembers.js"
    },
    {
      "src": "/api/.*",            // Default route for other API functions
      "dest": "/api/index.js"
    }
  ]
}
